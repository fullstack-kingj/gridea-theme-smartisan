<% if (pagination.prev || pagination.nex) { %>
  <div class="pagination-container">
    <% if (pagination.prev) { %>
      <a href="<%= pagination.prev %>" class="prev-btn">上一页</a>
      <% } %>
        <% if (pagination.next) { %>
          <a href="<%= pagination.next %>" class="next-btn">下一页</a>
          <% } %>
  </div>
  <% } %>